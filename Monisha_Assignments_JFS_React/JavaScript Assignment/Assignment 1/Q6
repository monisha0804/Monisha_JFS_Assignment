document.write("*******");

var n=["Hello","World","in" , "a","frame"];

function funString(n){
    for(var i=0; i<n.length;i++){
        document.write("<br>");
        document.write("* " +n[i] + " *");
        
    }
}
funString(n);
document.write("<br>");
document.write("*******");
